<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ligaODeLeticiaRejeitadaVM" type="com.application.app.modules.ligaodeleticiarejeitada.data.viewmodel.LigaODeLeticiaRejeitadaVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLigaodeLeti"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_background_11}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_background_11"/>
        <LinearLayout android:id="@+id/linearColumnletcia"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleteal_702">
            <ImageView android:id="@+id/imageImageviewkelly"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_110pxh"
                android:layout_marginEnd="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_97pxh"
                android:layout_marginTop="@dimen/_114pxv"
                android:layout_width="@dimen/_110pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_avatarleticia}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_avatarleticia"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLetcia"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_97pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtLetcia}"
                style="@style/txtRobotoregular32" tools:text="@string/lbl_let_cia"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNoatendida"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_97pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtNoatendida}"
                style="@style/txtRobotoregular18_5" tools:text="@string/lbl_n_o_atendida"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowclose"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_75pxh"
                android:layout_marginStart="@dimen/_75pxh"
                android:layout_marginTop="@dimen/_267pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearButtoncancelar"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.63"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_radius_36"
                        android:id="@+id/btnClose"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_72pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_72pxh"
                        android:paddingHorizontal="@dimen/_23pxh"
                        android:paddingVertical="@dimen/_23pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close_72x72" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCancelar"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtCancelar}"
                        style="@style/txtRobotolight12" tools:text="@string/lbl_cancelar"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearButtoncall"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.37"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_green_a705_radius_36"
                        android:id="@+id/btnVector"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_72pxh"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="@dimen/_72pxh"
                        android:paddingHorizontal="@dimen/_16pxh"
                        android:paddingVertical="@dimen/_16pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_white_a700" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLigarnovamente"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtLigarnovamente}"
                        style="@style/txtRobotolight12" tools:text="@string/lbl_ligar_novamente"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowvectorOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_56pxv"
                android:layout_marginEnd="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_97pxh"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearVideocapture"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:orientation="vertical">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_10"
                        android:id="@+id/viewVectorOne"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_2pxh" android:layout_width="@dimen/_20pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtSCounter}"
                        style="@style/txtRobotomedium12" tools:text="@string/lbl_30_s"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearClipvoicecapt"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.13"
                    android:layout_width="0dp" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackglobe"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_2pxh" android:layout_width="@dimen/_19pxh">
                        <ImageView android:id="@+id/imageGlobe"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_globe"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_globe"/>
                        <ImageView android:id="@+id/imageMicrophone"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_7pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_microphone_9x7"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_microphone_9x7"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSCounterOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{ligaODeLeticiaRejeitadaVM.ligaODeLeticiaRejeitadaModel.txtSCounterOne}"
                        style="@style/txtRobotomedium12" tools:text="@string/lbl_30_s"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
