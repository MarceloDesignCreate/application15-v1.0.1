<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pushNotificationVM" type="com.application.app.modules.pushnotification.data.viewmodel.PushNotificationVM"/>
    </data>
    <FrameLayout android:id="@+id/framePushNotificati"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackbackground"
            android:layout_gravity="start"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageBackground"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_background_5}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_background_5"/>
            <ImageView android:id="@+id/imageBackgroundbran"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_backgroundbran}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_backgroundbran"/>
        </FrameLayout>
        <ImageView android:id="@+id/imageScreenshot2021"
            android:layout_gravity="start"
            android:layout_height="@dimen/_822pxv"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_screenshot2021}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_screenshot2021"/>
        <LinearLayout android:id="@+id/linearColumn"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900_33">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_683pxv"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnvector"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_4">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_16x16"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_16x16"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtChamadadevoz"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.48"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pushNotificationVM.pushNotificationModel.txtChamadadevoz}"
                            style="@style/txtRobotoregular12_1" tools:text="@string/msg_chamada_de_voz"/>
                        <View
                            android:background="@drawable/rectangle_bg_teal_500_radius_1_335"
                            android:id="@+id/viewFiber"
                            android:layout_height="@dimen/_2pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_2pxh"/>
                        <Spinner
                            android:id="@+id/spinnerGroupSeventyNine"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.47"
                            android:layout_width="0dp" tools:listitem="@layout/spinner_item"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowletciavilella"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnletciavilella"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.87"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLetciaVilella"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{pushNotificationVM.pushNotificationModel.txtLetciaVilella}"
                                style="@style/txtRobotomedium14_1" tools:text="@string/lbl_let_cia_vilella"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSoPaulo"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{pushNotificationVM.pushNotificationModel.txtSoPaulo}"
                                style="@style/txtRobotoregular14_1" tools:text="@string/lbl_s_o_paulo"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageClose"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close_19x19"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close_19x19"/>
                        <View
                            android:background="@drawable/rectangle_bg_teal_500_radius_10"
                            android:id="@+id/viewVectorOne"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_20pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
    </FrameLayout>
</layout>
