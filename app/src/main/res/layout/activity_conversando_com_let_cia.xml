<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="conversandoComLetCiaVM" type="com.application.app.modules.conversandocomletcia.data.viewmodel.ConversandoComLetCiaVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackbackground"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_background_1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_background_1"/>
        <LinearLayout android:id="@+id/linearColumnappbarbgone"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <FrameLayout android:id="@+id/frameStackappbarbgone"
                    android:layout_height="@dimen/_88pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageAppbarbgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_88pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_appbarbg1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_appbarbg1"/>
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <LinearLayout
                            android:id="@+id/linearRowavatarleticia"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_weight="0.96"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageAvatarleticia"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_width="@dimen/_36pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_avatarleticia}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_avatarleticia"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumntextviewletci"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.7"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtTextviewLetci"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtTextviewLetci}"
                                    style="@style/txtRobotomedium18" tools:text="@string/lbl_let_cia"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtOnline"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginStart="@dimen/_1pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtOnline}"
                                    style="@style/txtRobotoregular13" tools:text="@string/lbl_online"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageVideocamera"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_14pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_videocamera"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_videocamera"/>
                            <ImageView android:id="@+id/imageVector"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_11pxv"
                                android:layout_marginStart="@dimen/_34pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                            <View android:background="@color/white_A700"
                                android:id="@+id/lineVectorOne"
                                android:layout_height="@dimen/_16pxv"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginStart="@dimen/_36pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.02" android:layout_width="0dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnbgmessage"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnmensagemNoLidaOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleblack_900_33">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnMensagemNoLidaOne"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_125pxh"
                        android:layout_marginStart="@dimen/_125pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/msg_mensagem_n_o_li"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/msg_mensagem_n_o_li"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameIncomingmessag"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_28pxv" android:layout_width="@dimen/_214pxh">
                    <ImageView android:id="@+id/imageBgmessage"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_31pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bgmessage"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bgmessage"/>
                    <LinearLayout
                        android:id="@+id/linearRowhellolorettaf"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHelloLorettaF"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.78"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtHelloLorettaF}"
                            style="@style/txtRobotoregular14" tools:text="@string/lbl_estou_chegando"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_61pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.22"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtTime}"
                            style="@style/txtRobotoregular11" tools:text="@string/lbl_16_33"/>
                    </LinearLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameOutgoingmessag"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_31pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_182pxh"
                    android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_219pxh">
                    <ImageView android:id="@+id/imageBgmessageOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_31pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bgmessage_light_green_100"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bgmessage_light_green_100"/>
                    <LinearLayout android:id="@+id/linearRowhininariley"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHiNinaRiley"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.69"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtHiNinaRiley}"
                            style="@style/txtRobotoregular14" tools:text="@string/msg_posso_te_ligar"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTimeOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_49pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.2"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtTimeOne}"
                            style="@style/txtRobotoregular11" tools:text="@string/lbl_16_40"/>
                        <ImageView android:id="@+id/imageCheck"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_check"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_check"/>
                    </LinearLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacknavbarbgone"
                    android:layout_height="@dimen/_48pxv"
                    android:layout_marginTop="@dimen/_509pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageNavbarbgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_48pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_navbarbg1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_navbarbg1"/>
                    <LinearLayout android:id="@+id/linearRowback"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageBack"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_back"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_back"/>
                        <View
                            android:background="@drawable/rectangle_border_white_a700_radius_8"
                            android:id="@+id/viewHome"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_95pxh"
                            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_16pxh"/>
                        <View
                            android:background="@drawable/rectangle_border_white_a700_radius_2"
                            android:id="@+id/viewRecent"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_95pxh"
                            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_16pxh"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameBottombar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_11pxh" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowellipseone"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowvolume"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStylewhite_1">
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtDigiteumamens"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_weight="0.69"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{conversandoComLetCiaVM.conversandoComLetCiaModel.txtDigiteumamens}"
                            style="@style/txtRobotoregular20" tools:text="@string/msg_digite_uma_mens"/>
                        <ImageView android:id="@+id/imageTrash"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_27pxh"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_trash"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_trash"/>
                        <ImageView android:id="@+id/imageCamera"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_camera"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                    </LinearLayout>
                    <View
                        android:background="@drawable/rectangle_bg_teal_500_radius_25"
                        android:id="@+id/viewEllipseOne"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_marginBottom="@dimen/_3pxv" android:layout_width="@dimen/_50pxh"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
